{
  "name": "regula",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.20.0",
    "allure-commandline": "^2.13.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/jasmine-framework": "^9.20.0",
    "@wdio/local-runner": "^9.20.0"
  },
  "scripts": {
  "regula": "wdio run ./wdio.conf.js",
  "test:all": "wdio run ./wdio.conf.js",
  "allure:generate": "allure generate ./allure-results --clean -o ./allure-report",
  "allure:open": "allure open ./allure-report",
  "test:e2e": "set TEST_SUITE=e2e&& wdio run ./wdio.conf.js",
  "test:api": "set TEST_SUITE=api&& wdio run ./wdio.conf.js",
  "test:smoke": "set TEST_SUITE=smoke&& wdio run ./wdio.conf.js"
  }
}
